    module BIOS

nothing:
    ld a, #ff
    ret

install_int:
    ld (.set_a),a
    ld (.set_hl), hl
    ld a, #0c3
    ld (#38),a
    ld hl, int_handler
    ld (#39), hl
    ld a,0
.set_a = $ -1
    ld hl, 0
.set_hl = $ - 2
    ret

bios_print:
    ld a, (hl) : cp #ff : ret z
    inc hl
    ld c, a
    push hl
    call Display.putc
    pop hl
    jr bios_print


old_sp:
    dw 0

    endmodule